    "node": ">=10.0.0"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build:app": "ng build --configuration production",
    "build:watch": "ng build --watch --project ng-bootstrap-form-validation",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "clean": "rimraf dist && rimraf .ng_build",
